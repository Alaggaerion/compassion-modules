      <field name="name">Test Template 1</field>
      <field name="layout">CH-A-1S11-1</field>
      <field name="pattern_image" type="base64" file="sbc_compassion/static/src/img/triforce.png"/>
      <field name="template_image" type="base64" file="sbc_compassion/static/src/img/template_test1.png"/>
      <!-- Just need to be larger than all the others values.
	   They will be computed during the initialization. -->
      <field name="page_width">2480</field>
      <field name="page_height">3507</field>
      <field name="pattern_x_min">1155</field>
      <field name="pattern_x_max">1393</field>
      <field name="pattern_y_min">3249</field>
      <field name="pattern_y_max">3343</field>
      <field name="checkbox_ids" eval="False"/>
    </record>
    
    <!-- Language checkboxes -->
    <record id="test_french_checkbox1" model="sponsorship.correspondence.lang.checkbox">
      <field name="template_id" ref="test_template1"/>
